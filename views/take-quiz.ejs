                                });
                            </script>
                        <% } else { %>
                            <!-- Regular Quiz Questions -->
                            <script>
                                console.log('üîç REGULAR QUIZ SECTION - About to render questions...');
                                console.log('Questions available:', <%= quiz.questions ? quiz.questions.length : 0 %>);

                                // Debug: Check questions after rendering
