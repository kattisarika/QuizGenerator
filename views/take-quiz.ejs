                                            var hexViewBox = '0 0 ' + (hexSize + 20) + ' ' + (hexSize + 20);
                                            var hexCenterX = (hexSize + 20) / 2;
                                            var hexCenterY = (hexSize + 20) / 2;
                                            var hexRadius = hexSize / 2;
                                            var hexPoints = [];
                                            for (var i = 0; i < 6; i++) {
